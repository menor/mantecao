{
  "name": "mantecao",
  "version": "0.3.0-0",
  "description": "A react UI library for resmio",
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "eslint . --ext .js --ext .jsx || true",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix || true",
    "start": "webpack-dev-server",
    "styleguide-build": "NODE_ENV=production styleguidist build",
    "styleguide-server": "styleguidist server",
    "test": "npm run lint && npm run test:only",
    "test:only": "NODE_ENV=production tape -r babel-register ./src/components/**/*.spec.js | faucet || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resmio/mantecao.git"
  },
  "keywords": [
    "react",
    "ui",
    "library",
    "components",
    "resmio"
  ],
  "author": "resmio <developers@resmio.com> (https://resmio.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resmio/mantecao/issues"
  },
  "homepage": "https://github.com/resmio/mantecao#readme",
  "peerDependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.14.0",
    "bourbon": "^4.2.7",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "faucet": "0.0.1",
    "html-webpack-plugin": "^2.21.0",
    "jsdom": "^9.4.2",
    "node-sass": "^3.8.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-styleguidist": "^3.1.1",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "webpack-node-externals": "^1.2.0",
    "webpack-validator": "^2.2.7"
  }
}
